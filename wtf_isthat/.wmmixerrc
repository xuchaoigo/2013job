#
# This is an example template. Copy this file to
# ~/.wmmixerrc and change any options you want different.
# For now there aren't many options to choose of :-)
#

[theme]
#file=/usr/local/share/wmmixer/traditional/theme.rc
file=/usr/local/share/wmmixer/lcd/theme.rc

[channels]
# Only display selected channels:
display=0,5,10

# Set volume of distinct channels
# volume0=55
# volume1=22

# Set different left and right values for a stereo channel
# volume_left2=0
# volume_right2=30


